generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Chat {
  id       String @id @default(uuid())
  name     String
  messages String
}

model Project {
  id          String @id @default(uuid())
  title       String
  description String
}

model Blog {
  id       String @id @default(uuid())
  title    String
  subtitle String
  first    String
  second   String
  third    String
  fourth   String
}
